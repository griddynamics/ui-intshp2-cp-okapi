{
  "name": "capstone-angular-project",
  "version": "0.0.0",
  "scripts": {
    "server": "node src/server/app.js",
    "server:watch": "nodemon node --inspect src/server/app.js",
    "server:debug": "node --inspect src/server/app.js",
    "start": "npm run serve:dev --live-reload=false",
    "build:prod": "ng build --prod --aot",
    "serve:prod": "http-server output/dist/",
    "serve:dev": "ng serve -o --configuration=test",
    "test": "ng test",
    "coverage": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "prepush": {
    "run": [
      "lint",
      "coverage"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.8",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "^7.2.0",
    "@angular/compiler": "^7.2.0",
    "@angular/core": "^7.2.0",
    "@angular/forms": "^7.2.0",
    "@angular/http": "7.1.4",
    "@angular/material": "^7.3.3",
    "@angular/platform-browser": "^7.2.0",
    "@angular/platform-browser-dynamic": "^7.2.0",
    "@angular/pwa": "^0.13.5",
    "@angular/router": "^7.2.0",
    "@angular/service-worker": "^7.2.0",
    "@ng-bootstrap/ng-bootstrap": "^4.0.1",
    "body-parser": "^1.15.2",
    "bootstrap": "^4.2.1",
    "core-js": "^2.6.2",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "intersection-observer": "^0.5.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.0",
    "@angular/cli": "^7.2.1",
    "@angular/compiler-cli": "^7.2.5",
    "@angular/language-service": "^7.2.0",
    "@types/jasmine": "^2.8.14",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.10.39",
    "codelyzer": "~4.5.0",
    "http-server": "^0.11.1",
    "husky": "^1.3.1",
    "intl": "^1.2.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "nodemon": "^1.18.9",
    "phantomjs-prebuilt": "^2.1.16",
    "pre-push": "^0.1.1",
    "protractor": "^5.4.2",
    "puppeteer": "^1.11.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
